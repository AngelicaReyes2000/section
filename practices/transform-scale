div {
  width: 150px;
  height: 150px;
  display: inline-block;
}
.one {
  background: red;
  transform: scaleX(0.5);
}
.two {
  background: green;
  transform: scaleY(1.5);
}
.three {
  background: blue;
  transform: scale(1.2, 1.5);
  transform: scale(2);
}
